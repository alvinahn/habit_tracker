<div class="row center"><h3>Welcome <%= session[:first_name] %></h3>
<br>
<form method="GET" action="/profile/signout">
  <button><input type="submit" value="signout"></button>
</form>

<% if @habit %>
  <% @habit.each do |habit| %>
    <div>
      <p><%= habit.name %></p>
    </div>
  <% end %>
<% else %>
<br>
<br>
  <p>You are not tracking any habits</p>
<% end %>
<br>

<div class="row center">
  <a href="/form"><p>Create</p></a>
</div>